{
  "name": "postman-to-api",
  "version": "1.0.2",
  "description": "CLI tool to generate API service files from Postman collections",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "postman-to-api": "dist/bin/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "ts-node bin/cli.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "postman",
    "api",
    "cli",
    "codegen",
    "typescript",
    "javascript",
    "axios",
    "fetch",
    "api-client"
  ],
  "author": "Karan <ansh.tsx@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anshkaran7/postman-to-api.git"
  },
  "bugs": {
    "url": "https://github.com/Anshkaran7/postman-to-api/issues"
  },
  "homepage": "https://github.com/Anshkaran7/postman-to-api#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "postman-collection": "^4.3.0",
    "prettier": "^3.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "@types/postman-collection": "^3.5.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
